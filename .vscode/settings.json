    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "vscode.markdown-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
        },
        "editor.formatOnSave": true,
        "editor.tabSize": 2
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    // Interface
    "breadcrumbs.filePath": "on",
    "breadcrumbs.icons": true,
    "breadcrumbs.symbolPath": "on",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.scrollBeyondLastLine": false,
    "editor.smoothScrolling": true,
    "window.title": "${dirty}${separator}${rootName}${separator}${activeEditorMedium}",
    "window.titleBarStyle": "custom",
    "workbench.editor.labelFormat": "short",
    "workbench.list.smoothScrolling": true,
    // Font
    "editor.fontLigatures": "true",
    "editor.fontSize": 16,
    "editor.fontWeight": "400",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.2,
    // Cursor
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "block",
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line",
    // IntelliSense
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.parameterHints.enabled": true,
    "editor.suggest.localityBonus": true,
    "editor.suggestOnTriggerCharacters": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": true,
    "editor.quickSuggestionsDelay": 1,
    // File settings
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.defaultLanguage": "",
    "files.encoding": "utf8",
    "files.eol": "\n", // formatting only supports LF line ending
    "files.hotExit": "onExit",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/node_modules": true
    },
    // explorer
    "explorer.compactFolders": false,
    "explorer.confirmDelete": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.decorations.badges": false,
    "explorer.openEditors.visible": 4,
    "explorer.sortOrder": "type",
    // terminal
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    // GIT
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "editor.codeLens": true,
    "editor.codeLensFontFamily": "Comic Sans MS",
    "editor.codeLensFontSize": 12,
    "git.autofetchPeriod": 60,
    "git.autorefresh": true,
    "git.suggestSmartCommit": false,
    // EDITOR
    "editor.autoIndent": "full",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.addMissingImports": true,
        "source.organizeImports": true,
        "source.fixAll.eslint": true
    },
    "editor.autoClosingQuotes": "beforeWhitespace",
    "editor.cursorWidth": 0,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.glyphMargin": true,
    "editor.highlightActiveIndentGuide": true,
    "editor.insertSpaces": true,
    "editor.letterSpacing": 1,
    "editor.lineHeight": 25,
    "editor.linkedEditing": true,
    "editor.links": false,
    "editor.matchBrackets": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.occurrencesHighlight": true,
    "editor.parameterHints.cycle": true,
    "editor.renderIndentGuides": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.showFoldingControls": "always",
    "editor.showUnused": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.showOperators": true,
    "editor.tabSize": 4,
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "editor.wordWrap": "on",
    "search.showLineNumbers": true,
    "telemetry.enableTelemetry": false,
    "workbench.colorTheme": "One Monokai",
    "workbench.editor.enablePreview": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tree.horizontalScrolling": true,
    "zenMode.centerLayout": false,
    "editor.rulers": [
        100
    ],
    "eslint.alwaysShowStatus": true,
    //HTML
    "html.autoClosingTags": true,
    "html.format.contentUnformatted": null,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.maxPreserveNewLines": 0,
    "html.format.unformatted": "",
    "html.format.wrapAttributes": "force-aligned",
    "html.format.wrapLineLength": 100,
    "html.format.enable": true,
    "html.format.templating": true,
    // CSS
    "css.completion.completePropertyWithSemicolon": true,
    "css.completion.triggerPropertyValueCompletion": true,
    "css.lint.duplicateProperties": "warning",
    "css.lint.idSelector": "warning",
    "css.lint.vendorPrefix": "error",
    "css.trace.server": "messages",
    "css.validate": true,
    //JavaScript
    "javascript.autoClosingTags": true,
    // "javascript.format.enable": true,
    // "javascript.format.insertSpaceAfterCommaDelimiter": true,
    // "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
    // "javascript.format.semicolons": "insert",
    "javascript.preferences.importModuleSpecifier": "relative",
    "javascript.preferences.quoteStyle": "single",
    "javascript.suggest.autoImports": true,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.suggest.enabled": true,
    "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
    "javascript.suggestionActions.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.validate.enable": true,
    // Ext Material Icon
    "material-icon-theme.activeIconPack": "angular",
    "material-icon-theme.folders.color": "#afafaf",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.folders.associations": {
        "app": "Global",
        "core": "Public",
        "environments": "Config",
        "shared": "Tools",
        "domain": "class",
        "ui": "views"
    },
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.opacity": 0.8,
    // MacOS Only Settings.
    "terminal.integrated.macOptionIsMeta": true,
    "workbench.fontAliasing": "auto",
    "workbench.statusBar.feedback.visible": false,
    //Ext Eslint
    "prettier.eslintIntegration": true,
    "eslint.run": "onType",
    "search.exclude": {
        "**/node_modules": true
    },
    "html-css-class-completion.enableEmmetSupport": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
    "breadcrumbs.enabled": true,
    "scm.defaultViewMode": "tree",
    "window.zoomLevel": 0,
}
